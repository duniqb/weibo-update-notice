## db.setting文件

url = jdbc:h2:tcp://127.0.0.1/~/h2/data/weibo
#url = jdbc:h2:tcp://127.0.0.1/D:/software/h2/data/weibo
user = root
pass = root

## 可选配置
# 是否在日志中显示执行的SQL
showSql = true
# 是否格式化显示的SQL
formatSql = false
# 是否显示SQL参数
showParams = true
# 打印SQL的日志等级，默认debug，可以是info、warn、error
sqlLevel = debug
